import java.util.*;
public class prog{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        ArrayList<String> arr=new ArrayList<>();
        int n=sc.nextInt();
        for(int i=0;i<n;i++){
            arr.add(sc.next());
        }
        ArrayList<String> rev=new ArrayList<>();
        for (int i=0;i<n;i++){
            rev.add(arr.get(arr.size()-i-1));
        }
        System.out.println("List before reversing :\n"+arr);
        System.out.println("List after reversing :\n"+rev);
    }
}
