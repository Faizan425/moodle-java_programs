import java.util.Scanner;

public class ExceptionHandling {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        try {
            // Step 1: Input the size of the array
            int size = scanner.nextInt();

            // Step 2: Initialize the array and input elements
            int[] arr = new int[size];
          
            for (int i = 0; i < size; i++) {
                arr[i] = scanner.nextInt();
            }

            // Step 3: Attempt division and handle ArithmeticException
            int result = arr[0] / arr[1]; 
           

            // Step 4: Access an element beyond the array limit
            System.out.println( arr[size]);

        } catch (ArithmeticException e) {
            System.out.println( "java.lang.ArithmeticException: "+e.getMessage());

        } catch (ArrayIndexOutOfBoundsException e) {
            System.out.println( "java.lang.ArrayIndexOutOfBoundsException: "+e.getMessage());

        } finally {
            // Step 5: Always executed block
            System.out.println("I am always executed");
        }

        scanner.close();
    }
}
